function calculateProduct(...numbers) { 
    let product = 1; 
    let count = 0;
    for (let i = 0; i < numbers.length; i++) {
        if(numbers[i]!=0){
            product *= numbers[i];
        }else{
            count++; 
        }
    }
    if (count==0){
            product =1;
        }
    return product; 
}
