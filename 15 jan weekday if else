//index variable is already given
if (index==1){
    console.log("Monday")
}
else if (index==2){
    console.log("Tuesday")
}
else if (index==3){
    console.log("Wednesday")
}
else if (index==4){
    console.log("Thursday")
}
else if (index==5){
    console.log("Friday")
}
else if (index==6){
    console.log("Saturday")
}
else if (index==7){
    console.log("Sunday")
}
else{
    console.log("Invalid")
}
